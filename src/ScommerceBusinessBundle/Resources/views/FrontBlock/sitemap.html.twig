<div class="sp-block-outer sp-block-outer-{{ data.block.type }} {{ data.block.class }}" {{ data.block.dataAttributes }}>
    {% if data.model.subtitle is not empty %}
        <div class="row">
            <div class="col-xs-12">
                <div class="section-title">{{ data.model.subtitle }}</div>
            </div>
        </div>
    {% endif %}
    <div class="row">
        <div class="col-xs-12">
            {% if 'no-title' not in data.block.class and data.title is defined and data.title is not empty %}
                <h2>{{ data.title }}</h2>
            {% endif %}

            <ul class="links">
                {% for item in data.sitemap %}
                    <li>
                        <a href="{{ item["url"] }}">{{ item["name"] }}</a>
                        {% if((item["sub_data"]) is defined) %}
                            <ul class="links">
                                {% for subCategory in item["sub_data"] %}
                                    <li><a href="{{ subCategory["url"] }}">{{ subCategory["name"] }}</a></li>
                                {% endfor %}
                            </ul>
                        {% endif %}
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>

    <div class="content">
        <div class="section items-options"></div>
        <div class="section items-grid no-padding grid-view"></div>
        <div class="section items-pager no-padding text-center"></div>
    </div>
    {# {{ render(controller('ScommerceBusinessBundle:Template:twigIncludeTwig', {'template':'Components/Form:contact_form.html.twig', 'data': {    }}, {})) }} #}
</div>
