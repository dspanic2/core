{% if attribute is defined %}{% apply spaceless %}
    {% if showLink == 1 %}
        {% if app.user.hasPrivilege(3,field_data.attributeSet.uid) or is_granted('ROLE_ADMIN') %}
            <a href="{{ path("page_view", {'id': field_data.id, 'url': field_data.attributeSet.attributeSetCode, 'type': 'form'}) }}">{% if field_data.isLegalEntity == 0 %}{% trans %}B2C{% endtrans %}{% else %}{% trans %}B2B{% endtrans %}{% endif %} - {{ field_data.name }}</a>
        {% elseif app.user.hasPrivilege(2,field_data.attributeSet.uid) or is_granted('ROLE_ADMIN') %}
            <a href="{{ path("page_view", {'id': field_data.id, 'url': field_data.attributeSet.attributeSetCode, 'type': 'view'}) }}">{% if field_data.isLegalEntity == 0 %}{% trans %}B2C{% endtrans %}{% else %}{% trans %}B2B{% endtrans %}{% endif %} - {{ field_data.name }}</a>
        {% else %}
            {{ field_data.name }}
        {% endif %}
    {% else %}
        {{ field_data.name }}
    {% endif %}
{% endapply %}{% endif %}
