<td {% if fieldClass is not empty %}class="{{ fieldClass }}"{% endif %} data-code="{{ attribute.attributeCode }}">
    <span {% if width is not empty %}style="width: {{ width }}px;"{% endif %}>
        {% set value_array = value|json_decode(constant('JSON_OBJECT_AS_ARRAY')) %}
        {% if value_array is not empty %}
            {% for key, val in value_array %}
                {{ key }} : {{ val|json_encode(constant('JSON_PRETTY_PRINT')) }}</br>
            {% endfor %}
        {% endif %}
    </span>
</td>