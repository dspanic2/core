<td {% if fieldClass is not empty %}class="{{ fieldClass }}"{% endif %}>
    <div class="container">
        <div class="starrating risingstar d-flex justify-content-center flex-row-reverse">
            <input type="radio" id="star5" name="rating{{ entity_id }}" disabled="true" value="5"
                   {% if value is not empty and value == 5 %}checked="checked"{% endif %} /><label for="star5"
                                                                                                   title="5 star"></label>
            <input type="radio" id="star4" name="rating{{ entity_id }}" value="4" disabled="true"
                   {% if value is not empty and value == 4 %}checked="checked"{% endif %}/><label for="star4"
                                                                                                  title="4 star"></label>
            <input type="radio" id="star3" name="rating{{ entity_id }}" value="3" disabled="true"
                   {% if value is not empty and value == 3 %}checked="checked"{% endif %}/><label for="star3"
                                                                                                  title="3 star"></label>
            <input type="radio" id="star2" name="rating{{ entity_id }}" value="2" disabled="true"
                   {% if value is not empty and value == 2 %}checked="checked"{% endif %}/><label for="star2"
                                                                                                  title="2 star"></label>
            <input type="radio" id="star1" name="rating{{ entity_id }}" value="1" disabled="true"
                   {% if value is not empty and value == 1 %}checked="checked"{% endif %}/><label for="star1"
                                                                                                  title="1 star"></label>
        </div>
    </div>
</td>