<div class="sp-block {{ data.block.class }}" {% if data.block.dataAttributes is defined and data.block.dataAttributes is not empty %}{{ data.block.dataAttributes|raw }}{% endif %}>
    <div class="sp-block-inner">
        <div class="panel panel-midnightblue">
            <div class="panel-body collapse in">
                {% if is_granted('ROLE_ADMIN') %}
                    <div class="panel-heading main-panel-heading">
                        <h5>
                            <div class="options">
                                {{ include('AppBundle:Includes:manage_block_buttons.html.twig') }}
                            </div>
                        </h5>
                    </div>
                {% endif %}
                <div class="row sp-margin-bottom-15">
                    <div class="col-sm-12">
                       <iframe class="sp-google-preview" src='https://docs.google.com/viewerng/viewer?url={{ app.request.getSchemeAndHttpHost() }}{{ data.attribute.folder }}{{ data.model.file }}&embedded=true'></iframe>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

