<div class="sp-block sp-funnel-wrapper {{ data.block.class }}" {% if data.block.dataAttributes is defined and data.block.dataAttributes is not empty %}{{ data.block.dataAttributes|raw }}{% endif %}>
    <div class="sp-block-inner">
        <div class="panel panel-midnightblue">
            <div class="panel-body collapse in">
                <div class="col-sm-12">
                    <div class="panel-heading main-panel-heading">
                        <h5>{{ data.block.title|trans }}
                            {% if is_granted('ROLE_ADMIN') %}
                                <div class="options">
                                    {{ include('AppBundle:Includes:manage_block_buttons.html.twig') }}
                                </div>
                            {% endif %}
                        </h5>
                    </div>
                </div>
                <div class="row sp-margin-bottom-15">
                    <div class="col-sm-12">
                        <div class="sp-funnel" data-type="funnel" id="funnel" data-data="{{ data.data }}">

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

