{% extends 'base.html.twig' %}

{% block body %}
    <div id="page-content">
        <div id='wrap'>
            <div class="container">
                <div class="sp-block">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 main-content">
                            <div class="panel panel-midnightblue">
                                <div class="panel-heading">
                                    <h4>{{ managed_entity_type|trans }} {% trans %}list{% endtrans %}</h4>
                                </div>
                                <div class="panel-body collapse in table-panel-body sp-fixed-header">
                                    <table cellpadding="0" cellspacing="0" border="0"
                                           class="table table-striped table-bordered datatables fixed-columns" id="table_s1_2"
                                           data-url="{{ path('get_' ~ managed_entity_type ~ '_list') }}" data-server="true" data-limit="50"
                                           data-search="false" data-filter="true" data-export="false" data-order-col="5"
                                           data-order-dir="desc" data-empty="false" data-callback-after='["fixedTableAfter"]'
                                           data-filter-buttons="false" data-before='false' data-loader="false"
                                           data-f-columns="false" data-fl-column="0" data-fr-column="0" data-table="user">
                                        <thead>
                                            <tr class="sp-table-width">
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                                <th class="sp-actions-td"></th>
                                            </tr>
                                            <tr class="sp-table-header">
                                                <th data-col-name="entityTypeCode" data-col-search="true" data-col-sort="true" data-col-filter="false">{% trans %}Entity type code{% endtrans %}</th>
                                                <th data-col-name="attributeSetCode" data-col-search="true" data-col-sort="true" data-col-filter="false">{% trans %}Attribute set code{% endtrans %}</th>
                                                <th data-col-name="entityId" data-col-search="true" data-col-sort="true" data-col-filter="false">{% trans %}Entitiy id{% endtrans %}</th>
                                                <th data-col-name="username" data-col-search="true" data-col-sort="true" data-col-filter="false">{% trans %}Username{% endtrans %}</th>
                                                <th data-col-name="action" data-col-search="true" data-col-sort="true" data-col-filter="false">{% trans %}Action{% endtrans %}</th>
                                                <th data-col-name="eventTime" data-col-search="true" data-col-sort="true" data-col-filter="false">{% trans %}Date{% endtrans %}</th>
                                                <th class="sp-actions-td" data-col-name="actions" data-col-search="false" data-col-sort="false" data-col-filter="false"><span class="sp-actions-td-inner">{% trans %}Actions{% endtrans %}</span></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div> <!-- container -->
        </div> <!--wrap -->
    </div> <!-- page-content -->
{% endblock %}
