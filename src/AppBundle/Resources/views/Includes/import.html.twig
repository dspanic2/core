<div class="modal fade" id="default_modal" role="dialog" aria-labelledby="label">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span>
                </button>
                <h5 class="modal-title">{% trans %}Import{% endtrans %} {{ attributeSet.attributeSetCode|trans }}</h5>
            </div>
            <div class="modal-body">
                <form data-validate="true" method="post" action="{{ path('import_xls',{'type': attributeSet.attributeSetCode }) }}" accept-charset="UTF-8"
                      data-callback='["custom"]'
                      data-custom-callback='["showImportResults"]'
                      class="form-horizontal row-border">
                    <div class="modal-header row">
                        <div class="col-sm-12 sp-dropzone-container">
                            <div class="dropzone-wrapper">
                                <span class="dropzone" data-validate-url="{{ path('import_validate_xls',{'type': attributeSet.attributeSetCode }) }}" data-callback='["validateImport"]' data-url="{{ path('file_upload_import', {'entity_type_code': attributeSet.entityType.entityTypeCode }) }}" data-limit="1"></span>
                            </div>
                        </div>
                        <div class="hidden">
                            <input type="hidden" name="path" value=""/>
                        </div>
                        <div class="col-sm-12 sp-matching-container" style="display: none;"></div>
                        <div class="col-sm-12 sp-results-container" style="display: none;"></div>
                    </div>
                    <div class="panel-footer">
                        <div class="row">
                            <div class="col-sm-7 col-sm-offset-2">
                                <div class="btn-toolbar">
                                    <button style="display: none;" type="submit" class="btn-primary btn btn-blue" data-action="stay">{% trans %}Import{% endtrans %}</button>
                                    <button type="button" class="btn-default btn pull-right btn-red" data-dismiss="modal"><span aria-hidden="true">{% trans %}Close{% endtrans %}</span></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>